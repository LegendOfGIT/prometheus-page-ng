{
  "name": "prometheus-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/prometheus-page/server/main.js",
    "start:dev": "ng run prometheus-page:serve-ssr",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "coverage": "jest --coverage",
    "test": "jest --watch",
    "dev:ssr": "ng run prometheus-page:serve-ssr",
    "serve:ssr": "node dist/prometheus-page/server/main.js",
    "build:ssr": "ng build && ng run prometheus-page:server",
    "prerender": "ng run prometheus-page:prerender"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@angular-slider/ngx-slider": "^2.0.3",
    "@nguniversal/express-engine": "^14.0.0",
    "@types/uuid": "^8.3.4",
    "bootstrap": "^5.2.1",
    "chart.js": "^4.0.0",
    "express": "^4.15.2",
    "ngx-cookie-service": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "^14.0.0",
    "@nguniversal/builders": "^14.0.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^14.15.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "typescript": "~4.7.2"
  }
}
